<body>
<header class="header">
    <div class="container">
        <nav>
            <div class="logo">
                <a href="/">
                    <img height="40" src="{{ asset('build/images/logo.png') }}" alt="discover-georgia">
                    <span>Discover Georgia</span>
                </a>
            </div>
            <ul class="header__menu">
                <li><a href="#">О нас</a></li>
                <li><a href="#">Туры</a></li>
                <li><a href="#">Компаниям</a></li>
            </ul>
        </nav>
        <div class="header__social-media">
            <div class="header__social-media_links">
                <a href="tel:+74951234567">+995 123-45-67</a>
                <a href="https://t.me/your_name" target="_blank"
                   title="Написать в Telegram">
                    <svg class="icon">
                        <use xlink:href="#telegram"></use>
                    </svg>
                </a>
                <a href="https://t.me/your_name" target="_blank"
                   title="Написать в Whatsapp">
                    <svg class="icon">
                        <use xlink:href="#whatsapp"></use>
                    </svg>
                </a>
            </div>
            <div class="header__currencies">
                <svg class="icon">
                    <use xlink:href="#flag-ge"></use>
                </svg>
                <a href="#">
                    GEL ^
                </a>
            </div>
        </div>

    </div>
</header>

<main id="app">

    {% block main %}

    {% endblock %}
</main>

<footer class="footer">

</footer>

